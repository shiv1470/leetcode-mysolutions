/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode* swapPairs(ListNode* head) {
        if(head==NULL)
        {
            return NULL;
        }
        ListNode *temp=head,*nx=head->next;
        if(nx==NULL)
        {
            return head;
        }
        temp->next=swapPairs(nx->next);
        nx->next=temp;
        return nx;
    }
};
